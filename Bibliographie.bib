address = {Mont-Dore, France},
author = {Laroche, Florent and {Kerouanton Jlk}, Jean-Louis and Servi\`{e}res, Myriam and Bernard, Alain},
booktitle = {12e colloque national AIP Primeca},
keywords = {NTIC,gestion des connaissances,inter-disciplinarit\'{e},maquette num\'{e}rique,patrimoine,p\'{e}dagogie,r\'{e}alit\'{e} virtuelle},
pages = {11 p.},
title = {{Un renouveau des pratiques mus\'{e}ographiques gr\^{a}ce au virtuel : un support \`{a} des applications pluridisciplinaire en p\'{e}dagogie active}},
url = {http://hal.archives-ouvertes.fr/hal-00585088},
year = {2011}
}
